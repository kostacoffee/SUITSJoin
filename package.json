{
  "name": "vuejs-template",
  "description": "A template project for vuejs webapps using material design",
  "author": "Kosta Dunn <kostacoffee@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --host 0.0.0.0 --inline --hot --content-base src/",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules && cp src/index.html public/."
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "node-sass": "^4.0.0",
    "pug": "^2.0.0-beta6",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "vue": "^2.0.1",
    "vue-loader": "^9.7.0",
    "vue-material": "^0.4.1",
    "vue-resource": "^1.0.3",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.0"
  }
}
